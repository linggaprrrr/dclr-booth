{
  "name": "dclr-photoshoot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/server/index.ts",
    "dev:worker": "ts-node --project tsconfig.server.json src/server/worker.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:worker\"",
    "build": "next build",
    "start": "NODE_ENV=production ts-node --project tsconfig.server.json src/server/index.ts",
    "start:worker": "NODE_ENV=production ts-node --project tsconfig.server.json src/server/worker.ts",
    "start:all": "concurrently \"npm run start\" \"npm run start:worker\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@bull-board/api": "^6.9.1",
    "@bull-board/express": "^6.9.1",
    "axios": "^1.8.2",
    "body-parser": "^1.20.2",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.3",
    "formdata-node": "^6.0.3",
    "ioredis": "^5.6.0",
    "next": "15.2.0",
    "qr-scanner": "^1.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.0",
    "concurrently": "^9.1.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
